{
  "ci": {
    "upload": {
      "target": "filesystem"
    },
    "collect": {
      "numberOfRuns": 10
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.1}],
        "categories:accessibility": ["error", {"minScore": 0.1}],
        "categories:best-practices": ["error", {"minScore": 0.1}],
        "categories:seo": ["error", {"minScore": 0.8}],
        "csp-xss": "warn"
      }
    }
  }
}